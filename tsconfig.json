{
  "compilerOptions": {
      "resolveJsonModule": true,
      "allowJs": true,
      "sourceMap": true,
      "target": "ES2020",
      "module": "CommonJS",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./src",
      "baseUrl": ".",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "paths": {
          "@/*": ["src/*"]
      }
  },
  "include": [
      "src/**/*.ts"
  ],
  "exclude": ["node_modules"],
  "suppressImplicitAnyIndexErrors": true,
  "noImplicitAny": false
}